{
  "compilerOptions": {
    "esModuleInterop": true,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "paths": {
      "@exp/app/*": [
        "./packages/app/src/*"
      ],
      "@exp/ui/*": [
        "./packages/ui/src/*"
      ]
    }
  },
  "extends": "./tsconfig.base",
  "exclude": [
    "**/node_modules",
    "**/dist",
    "**/types",
    "apps/next/out",
    "apps/next/.next",
    "apps/next/.tamagui"
  ]
}